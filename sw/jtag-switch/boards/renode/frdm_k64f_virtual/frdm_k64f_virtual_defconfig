# Serial console (UART0 instead of USB CDC ACM)
CONFIG_CONSOLE=y
CONFIG_UART_CONSOLE=y
CONFIG_SERIAL=y

# GPIO via emulator
CONFIG_GPIO=y
CONFIG_GPIO_EMUL=y

# System clock (match K64F)
CONFIG_SYS_CLOCK_HW_CYCLES_PER_SEC=120000000

# Oscillator (match real hardware for driver compatibility)
CONFIG_OSC_EXTERNAL=y

# Enable ARM MPU
CONFIG_ARM_MPU=y
