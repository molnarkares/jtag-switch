# FRDM-K64F Virtual Board (Renode)
# SPDX-License-Identifier: Apache-2.0

if BOARD_FRDM_K64F_VIRTUAL

# Oscillator and clock configuration (same as real FRDM-K64F)
config OSC_XTAL0_FREQ
	default 50000000

config MCG_PRDIV0
	default 0x13

config MCG_VDIV0
	default 0x18

config MCG_FCRDIV
	default 1

if NETWORKING

config NET_L2_ETHERNET
	default y if !MODEM

endif # NETWORKING

endif # BOARD_FRDM_K64F_VIRTUAL
